<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<VOTABLE xmlns="http://www.ivoa.net/xml/VOTable/v1.3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.4" xsi:schemaLocation="http://www.ivoa.net/xml/VOTable/v1.3 http://www.ivoa.net/xml/VOTable/v1.3">
    <RESOURCE type="results">
        <RESOURCE type="meta">
            <VODML xmlns="https://ivoa.net/xml/MIVOT">
                <REPORT status="OK">hand-made mapping: ad hoc model so far</REPORT>
                <MODEL name="instfov" url="https://www.ivoa.net/xml/instfov.xml"/>
                <MODEL name="coords" url="https://www.ivoa.net/xml/Coords-v1.xsd"/>
                <GLOBALS> <!-- instrument and telescope information -->
                <INSTANCE dmid="_universalCoordSys" dmtype="coords:SpaceSys">
                        <INSTANCE dmrole="coords:SpaceSys.spaceFrame" dmtype="coords:SpaceFrame">
                            <INSTANCE dmrole="coords:SpaceFrame.refPosition" dmtype="coords:stdRefLocation">
                                <ATTRIBUTE dmrole="coords:stdRefLocation.position" dmtype="ivoa:string" value="BARYCENTER"/>
                            </INSTANCE>
                            <ATTRIBUTE dmrole="coords:SpaceFrame.spaceRefFrame" dmtype="ivoa:string" value="FK5"/>
                        </INSTANCE>
                </INSTANCE>
                <INSTANCE dmrole="root" dmtype="instfov:InstrumentalFootPrint">
                    <ATTRIBUTE dmrole="instfov:author_name" dmtype="ivoa:string" value="STScI/T. Donaldson"/>
                    <ATTRIBUTE dmrole="instfov:author_contact_email" dmtype="ivoa:string" value="NotSet"/>
                    <COLLECTION dmrole="instfov:telescope">
                        <INSTANCE dmid="tel_id" dmtype="instfov:Telescope">
                            <ATTRIBUTE dmrole="instfov:Telescope.name" dmtype="ivoa:string" value="HST"/>
                            <ATTRIBUTE dmrole="instfov:Telescope.description" dmtype="ivoa:string" value="IUVIS+IUVIS1+IUVIS2"/>
                            <COLLECTION dmrole="instfov:instrument_host">
                                <INSTANCE dmid="inst_id" dmtype="instfov:Instrument">
                                    <ATTRIBUTE dmrole="instfov:Instrument.name" dmtype="ivoa:string" value="WFC3.IUVISBOTH"/>
                                    <ATTRIBUTE dmrole="instfov:description" dmtype="ivoa:string" value="IUVIS+IUVIS1+IUVIS2"/>
                                    <INSTANCE dmid="instmode_id" dmtype="instfov:InstrumentMode">
                                        <ATTRIBUTE dmrole="instfov:Instrument.name" dmtype="ivoa:string" value="WFC3.IUVISBOTH"/>
                                        <ATTRIBUTE dmrole="instfov:description" dmtype="ivoa:string" value="IUVIS+IUVIS1+IUVIS2"/>
                                        <!-- FOV for this instrument Mode -->
                                        <INSTANCE dmid="_fov" dmtype="instfov:Fov">
                                            <COLLECTION dmrole="instfov:FoV.shape">
                                                <INSTANCE dmrole="instfov:Fov.shape" dmtype="instfov:Polygon">
                                                    <COLLECTION dmrole="instfov:Polygon.vertex">
                                                        <INSTANCE dmtype="coords:LonLatPoint">
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lon" dmtype="ivoa:real" value="-0.0129374545"/>
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lat" dmtype="ivoa:real" value="0.018800126"/>
                                                        </INSTANCE>
                                                        <INSTANCE dmtype="coords:LonLatPoint">
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lon" dmtype="ivoa:real" value="0.016985504"/>
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lat" dmtype="ivoa:real" value="-0.015113308"/>
                                                        </INSTANCE>
                                                        <INSTANCE dmtype="coords:LonLatPoint">
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lon" dmtype="ivoa:real" value="9.931425E-4"/>
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lat" dmtype="ivoa:real" value="-0.031010153"/>
                                                            <REFERENCE dmref="_custcoordsys" dmrole="coords:LonLatPoint.coordSys"/>
                                                        </INSTANCE>
                                                        <INSTANCE dmtype="coords:LonLatPoint">
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lon" dmtype="ivoa:real" value="-0.028568486"/>
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lat" dmtype="ivoa:real" value="0.0028843714"/>
                                                            <REFERENCE dmref="_custcoordsys" dmrole="coords:LonLatPoint.coordSys"/>
                                                        </INSTANCE>
                                                    </COLLECTION>
                                                </INSTANCE>
                                                <INSTANCE mrole="instfov:FoV.shape" dmtype="instfov:Polygon">
                                                    <COLLECTION dmrole="instfov:Polygon.vertex">
                                                        <INSTANCE dmtype="coords:LonLatPoint">
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lon" dmtype="ivoa:real" value="0.0031012846"/>
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lat" dmtype="ivoa:real" value="0.035145245"/>
                                                        </INSTANCE>
                                                        <INSTANCE dmtype="coords:LonLatPoint">
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lon" dmtype="ivoa:real" value="0.03341686"/>
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lat" dmtype="ivoa:real" value="0.0012370894"/>
                                                        </INSTANCE>
                                                        <INSTANCE dmtype="coords:LonLatPoint">
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lon" dmtype="ivoa:real" value="0.017248388"/>
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lat" dmtype="ivoa:real" value="-0.014866891"/>
                                                            <REFERENCE dmref="_custcoordsys" dmrole="coords:LonLatPoint.coordSys"/>
                                                        </INSTANCE>
                                                        <INSTANCE dmtype="coords:LonLatPoint">
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lon" dmtype="ivoa:real" value="-0.012699403"/>
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lat" dmtype="ivoa:real" value="0.019020567"/>
                                                            <REFERENCE dmref="_custcoordsys" dmrole="coords:LonLatPoint.coordSys"/>
                                                        </INSTANCE>
                                                    </COLLECTION>
                                                </INSTANCE>
                                            </COLLECTION>
                                            <INSTANCE dmid="_custcoordsys" dmrole="instfov:Fov.customSpaceSys" dmtype="instfov:CustomSpaceSys">
                                                <INSTANCE dmrole="instfov:CustomSpaceSys.customSpaceFrame" dmtype="instfov:CustomSpaceFrame">
                                                    <INSTANCE dmrole="instfov:CustomSpaceFrame.refPosition" dmtype="instfov:CustomRefLonLatLocation">
                                                        <INSTANCE dmrole="instfov:CustomRefLonLatLocation.position" dmtype="coords:LonLatPoint">
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lon" dmtype="ivoa:real" value="NaN"/>
                                                            <ATTRIBUTE dmrole="coords:LonLatPoint.lat" dmtype="ivoa:real" value="NaN"/>
                                                            <REFERENCE dmref="_universalCoordSys" dmrole="instfov:standardSpaceReferenceSystem"/>
                                                        </INSTANCE>
                                                    </INSTANCE>
                                                <ATTRIBUTE dmrole="coords:SpaceFrame.spaceRefFrame" dmtype="ivoa:string" value="FK5"/>
                                                <ATTRIBUTE dmrole="coords:SpaceFrame.equinox" dmtype="ivoa:string" value="J2000"/>
                                                <ATTRIBUTE dmrole="instfov:CustomSpaceFrame.directionAngle" dmtype="ivoa:real" value="NaN"/>
                                                <ATTRIBUTE dmrole="instfov:CustomSpaceFrame.projection" dmtype="ivoa:string" value="CARTESIAN"/>
                                            </INSTANCE>
                                        </INSTANCE>
                                    </INSTANCE>
                                    <!-- other instrument modes used for this instrument come here as Instances-->
                                </INSTANCE> <!-- other instruments monted on this telescope come here -->
                            </COLLECTION>
                        </INSTANCE>
                    </COLLECTION>
                </INSTANCE>  
                </GLOBALS>
            </VODML>
        </RESOURCE>
    </RESOURCE>
</VOTABLE>
